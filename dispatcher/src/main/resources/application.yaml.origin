server:
  port: ${DISPATCHER_PORT:8090}
  ssl:
    enabled: false

bot:
  uri: ${BOT_URI}
  username: ${BOT_USERNAME}
  token: ${BOT_TOKEN}

spring:
  kafka:
    bootstrap-servers: ${BOOTSTRAP_SERVERS:localhost:9092}
    topics:
      answer-topic: ${ANSWER_TOPIC}
      edit-topic: ${EDIT_TOPIC}
      text-update-topic: ${TEXT_UPDATE_TOPIC}
      callback-query-update-topic: ${CALLBACK_QUERY_UPDATE_TOPIC}

